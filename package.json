{
  "name": "video-hoarder",
  "version": "0.0.7",
  "description": "youtube-dl frontend for NAS devices.",
  "main": "server/index.js",
  "scripts": {
    "clean": "rm -rf ./download/*; rm -rf ./build/*",
    "build-prod": "webpack --config webpack.config.prod.js",
    "build-docker": "npm run clean; docker build -t video-hoarder .",
    "run-docker": "docker rm video-hoarder; docker run -p 4000:4000 -v $(pwd)/download:/app/download --name video-hoarder video-hoarder",
    "lint": "eslint --ext .mjs --ext .js --ext .jsx server/ client/",
    "debug": "NODE_ENV=development node --inspect --experimental-modules server/index.mjs",
    "dev": "NODE_ENV=development node --experimental-modules server/index.mjs",
    "start": "node --experimental-modules server/index.mjs",
    "test": "echo \"Error: no test specified\" && exit 1",
    "publish": "git tag -a $npm_package_version"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akshat1/video-hoarder.git"
  },
  "author": "akshat.sh@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/akshat1/video-hoarder.git/issues"
  },
  "homepage": "https://github.com/akshat1/video-hoarder.git#readme",
  "dependencies": {
    "blueimp-md5": "^2.12.0",
    "bootstrap": "^4.4.1",
    "classnames": "^2.2.6",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-jsx-control-statements": "^4.0.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-plugin-jsx-control-statements": "^2.2.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-react": "^7.19.0",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "react": "^16.13.0",
    "react-bootstrap": "^1.0.0-beta.17",
    "react-dom": "^16.13.0",
    "react-hot-loader": "^4.12.20",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "style-loader": "^1.1.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  }
}
