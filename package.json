{
  "name": "video-hoarder",
  "version": "0.1.0-rc1",
  "description": "youtube-dl frontend for NAS devices.",
  "scripts": {
    "analyse-bundle": "webpack-bundle-analyzer -h 0.0.0.0 -O webpack-stats.json",
    "build-docker": "docker build -t video-hoarder .",
    "build-storybook": "build-storybook -o docs/storybook",
    "build": "NODE_ENV=production gulp build",
    "clean": "gulp clean",
    "dev": "npm run mkdir; NODE_ENV=development gulp dev; NODE_ENV=development node --inspect=0.0.0.0 --trace-warnings app/server/index.js",
    "docs": "mkdir -p docs; cp docs.html docs/index.html; npm run typedoc; npm run build-storybook",
    "typedoc": "typedoc src",
    "lint": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx --ext .cjs src/",
    "mkdir": "mkdir -p db-data",
    "publish": "git tag -a $npm_package_version",
    "start": "npm run build; npm run mkdir; NODE_ENV=production node app/server/index.js",
    "storybook": "start-storybook -p 6006",
    "test": "npm run mkdir; TZ=UTC NODE_ENV=test DEBUG=jest jest --coverage --silent=false"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akshat1/video-hoarder.git"
  },
  "author": "Akshat Sharma <akshat.sh@gmail.com>",
