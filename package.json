{
  "name": "video-hoarder",
  "version": "0.0.7",
  "description": "youtube-dl frontend for NAS devices.",
  "scripts": {
    "docs": "mkdir -p docs; cp docs.html docs/index.html; npm run jsdoc; npm run build-storybook",
    "jsdoc": "jsdoc -c jsdoc.conf.json",
    "clean": "rm -rf dist/* docs/",
    "dev": "NODE_ENV=development node src/server/index.js",
    "start": "npm run clean; npm run build; NODE_ENV=production node src/server/index.js",
    "build": "NODE_ENV=production webpack --config webpack.config.cjs",
    "lint": "eslint src/",
    "build-dev-docker": "docker build -t vh-dev-img .",
    "run-dev-docker": "docker run --rm -it --name vh-dev --network=\"host\" -v $PWD:/app/ vh-dev-img -v $PWD/db:/data/db/ -p 7200:7200",
    "test": "TZ=UTC NODE_ENV=test jest --coverage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o docs/storybook"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akshat1/video-hoarder.git"
  },
  "author": "akshat.sh@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/akshat1/video-hoarder.git/issues"
  },
  "homepage": "https://github.com/akshat1/video-hoarder.git#readme",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-regular-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "classnames": "^2.2.6",
    "express": "^4.17.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@storybook/addon-a11y": "^5.3.18",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addon-viewport": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/cli": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "babel-jest": "^25.2.4",
    "babel-loader": "^8.1.0",
    "babel-plugin-jsx-control-statements": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-control-statements": "^2.2.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.19.0",
    "extract-loader": "^5.0.1",
    "file-loader": "^6.0.0",
    "html-loader": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.2.4",
    "jsdoc": "^3.6.3",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "react-hot-loader": "^4.12.20",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.3",
    "toast-jsdoc": "^1.0.2",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.10.3",
    "webpack-hot-middleware": "^2.25.0"
  }
